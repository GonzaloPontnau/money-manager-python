{
  "version": 2,
  "builds": [
    {
      "src": "money_manager/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/staticfiles/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "money_manager/wsgi.py"
    }
  ]
}
